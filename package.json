{
  "name": "rs-infinite-scroll",
  "version": "0.1.0",
  "description": "An attempt at doing infinite scroll correctly for AngularJS",
  "main": "src/rs-infinite-scroll.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors src/rs-infinite-scroll.js bin/rs-infinite-scroll.js",
    "build-min": "NODE_ENV=production COMPRESS=1 webpack --progress --colors src/rs-infinite-scroll.js bin/rs-infinite-scroll.min.js",
    "demo": "webpack-dev-server --host 0.0.0.0 --config demo/webpack.config.js --no-info --content-base demo",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RetroSquareGroup/rs-infinite-scroll.git"
  },
  "keywords": [
    "infinite",
    "scroll",
    "javascript"
  ],
  "author": "E Brent Nelson <ebrentnelson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RetroSquareGroup/rs-infinite-scroll/issues"
  },
  "devDependencies": {
    "webpack": "~1.12.1",
    "babel-core": "~5.8.23",
    "babel-loader": "~5.3.2",
    "eslint": "~1.3.1",
    "eslint-loader": "~1.0.0",
    "babel-eslint": "~4.1.1",
    "ng-annotate-loader": "0.0.10",
    "webpack-dev-server": "~1.10.1",
    "angular": "~1.4.5"
  }
}
