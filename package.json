  },
  "devDependencies": {
    "angular": "1.7.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-angularjs-annotate": "^0.9.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.1.0",
    "ng-annotate-loader": "^0.6.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
